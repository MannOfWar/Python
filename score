score = input ("Insert your score")
if score>=80 and score<=100:
    print ("You got A")
elif score>=60 and score<=79:
    print ("You got B")
elif score>=40 and score<=59:
    print ("You got C")
elif score>=20 and score<=39:
    print ("You got D")
elif score>=0  and score<=19:
    print ("You got E")
